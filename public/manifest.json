{
  "manifest_version": 3,
  "name": "Voice Input Extension",
  "version": "1.0",
  "description": "Takes voice input using Web Speech API.",
  "permissions": ["activeTab", "storage"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
  }
}